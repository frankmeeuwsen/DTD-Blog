---
layout: default
---
{% if page.header %}
<img src="{{ site.url }}{{site.imageurl}}{{ page.header}}">
{% endif  %}
<article class="post">
  <h1>{{ page.title }}</h1>
  <div class="date">
    {{ page.date | date: "%e %B %Y" }}
  </div>

  <div class="entry">
    {{ content }}
  </div>


  {% include webmentions.html %}

</article>
